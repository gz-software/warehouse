<!-- EXCEL导出数据（Export  Modal） -->

<div class="modal fade" id="exportModal" tabindex="-1" role="dialog" aria-labelledby="outModalLabel" aria-hidden="true">
    <div class="modal-dialog" style="width:1200px;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="epModalLabel">
                   @LangHelper.Get("Excel_Label_Export"):  &nbsp;<span id="epEffNameSpan">@LangHelper.Get("Common_Label_NotSelected")</span>

                </h4>
            </div>

            <div class="modal-body">
                <form id="epForm" action="" enctype="multipart/form-data" class="container">

                    <input type="hidden" name="submitFileData.AssType" id="epAssTypeInput" value="0" />
                    <input type="hidden" name="submitFileData.DataType" id="epDataTypeInput" value="0" />

                    <div class="form-group" style="height:50px;">
                        <label class="col-sm-2 control-label">@LangHelper.Get("FileData_Entity_MapRule")</label>
                        <div class="col-sm-4">
                            <select id="epMapRuleSelect" name="submitFileData.MapRuleID" class="form-control"></select>
                        </div>


                        <label class="col-sm-2 control-label">@LangHelper.Get("FileData_Entity_MapRuleDescription")</label>
                        <div class="col-sm-4" id="epMapRuleDescDiv">&nbsp;</div>

                    </div>

                    <div class="form-group">
                        <label class="col-sm-2 control-label">@LangHelper.Get("FileData_Entity_FileName")</label>
                        <div class="col-sm-4 input-group">
                            <input id="epFileNameInput" type="text" name="submitFileData.FileName" class="form-control" />
                        </div>

                        <label class="col-sm-2 control-label">@LangHelper.Get("FileData_Entity_FileType")</label>
                        <div class="col-sm-4">
                            <select id="epFileTypeSelect" name="submitFileData.FileType" class="form-control"></select>
                        </div>
                    </div>

                    <div style="height:80px">&nbsp;</div>


                    <div class="form-group">
                        <label class="col-sm-2 control-label">@LangHelper.Get("FileData_Label_SelectMapRuleItem")</label>
                        <div class="col-sm-10" style="overflow-y: hidden; overflow-x: auto;  border: 1px solid #e5e5e5;">
                            <div id="epMapRuleOptionListDiv" style=" float: left; width: 40%; overflow-y: auto; height: 200px; background-color:#e5e5e5">
                            </div>
                            <div style="width:2%;float:left;">
                                &nbsp;
                            </div>
                            <div id="epMapRuleItemListDiv" style=" float: left; width: 58%; overflow-y: auto; height: 200px; ">
                            </div>
                        </div>
                    </div>

                  

                </form>
            </div><!-- /.modal-body -->

            <div class="modal-footer">
                <button type="button" class="btn btn-success" id="epUploadBtn"  onclick="epSubmit()">@LangHelper.Get("Excel_Button_Export")</button>
                <button type="button" class="btn btn-default" data-dismiss="modal" id="epCloseBtn">@LangHelper.Get("Common_Button_Close")</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->